            %td
              .btn-group
                %a.btn.btn-primary.btn-raised.btn-xs{:href=>"{{*escortstypelan_edit:id|$escorttypelang->id}}"}
                  %i.fa.fa-pencil
                %a.btn.btn-warning.btn-raised.btn-xs.showdelete{:href=>"{{*escortstypelan_delete:id|$escorttypelang->id}}"}
                  %i.fa.fa-trash
          {{endforeach}}


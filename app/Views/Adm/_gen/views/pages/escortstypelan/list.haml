      %table#items_list.table.table-striped.table-bordered.dataTable.no-footer{:cellspacing=>"0"}
        %thead
          %tr
            %th {{e. Id}}
            %th {{e. Item}}
            %th {{e. Acciones}}
        %tbody
          {{foreach $escortstypelan as $escorttypelang}}
          %tr{:id=>"item{{$escorttypelang->id}}"}
